{
    "name": "kauth/login",
    "description": "simple login",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Simple\\Login\\": "src/",
            "Simple\\Login\\Database\\": "src/database",
            "Simple\\Login\\App\\Models\\": "src/app/Models"
        }
    },
    "authors": [
        {
            "name": "KiemNV",
            "email": "kiemnv@kaiyouit.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "tymon/jwt-auth": "^1.0"
    },
    "require-dev": {
        "fakerphp/faker": "^1.9.1"
    },
    "scripts": {
        "post-install-cmd": [
            "php artisan vendor:publish --provider='Simple\\Login\\SimpleLoginServiceProvider'"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Simple\\Login\\SimpleLoginServiceProvider"
            ]
        }
    }
}
